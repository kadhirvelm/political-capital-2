{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsconfig.json"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {},
    "lint": {
      "dependsOn": [
        "build"
      ]
    },
    "lint:fix": {},
    "test": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
